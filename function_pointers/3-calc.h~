#ifndef _SAMPLE_HEADER_H_
#define _SAMPLE_HEADER_H_
#include <stdio.h>

/**                                                                                                                 
 * struct op - Struct op                                                                                            
 *                                                                                                                  
 * @op: The operator                                                                                                
 * @f: The function associated                                                                                      
 */
typedef struct op
{
    char *op;
    int (*f)(int a, int b);
} op_t;



int (*get_op_func(char *s))(int, int);
















#endif
